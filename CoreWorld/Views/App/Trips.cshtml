@{
    ViewBag.Tittle = "Home page";
}
@section scripts{
    <script src="~/lib/underscore/underscore-min.js"></script>
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/lib/angular-route/angular-route.min.js"></script>
    <script src="~/lib/gmaps/gmaps.min.js"></script>
    <script src="~/lib/travelmap/travelmap.min.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=&callback=initMap" type="text/javascript"></script>
    <environment names="Development">
        <script src="~/js/simpleController.js"></script>
        <script src="~/js/app-trips.js"></script>
        <script src="~/js/tripsController.js"></script>
        <script src="~/js/tripEditorController.js"></script>
    </environment>

    <environment names="Production, Staging">
        <script src="~/js/_app/simpleController.js"></script>
        <script src="~/js/_app/app-trips.js"></script>
        <script src="~/js/_app/tripsController.js"></script>
        <script src="~/js/_app/tripEditorController.js"></script>
    </environment>

}
<div class="row" ng-app="app-trips">
    <div ng-view></div>
</div>